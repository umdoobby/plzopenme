{
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "===> {Timestamp:HH:mm:ss} [{Level}] [{SourceContext}] [{EventId}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": "52428800",
          "rollOnFileSizeLimit": "true",
          "retainedFileCountLimit": "100",
          "retainedFileTimeLimit": "7.00:00:00",
          "outputTemplate": "===> {Timestamp:HH:mm:ss} [{Level}] [{SourceContext}] [{EventId}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Urls" : [
    "http://127.0.0.1:6981"
  ],
  "MariaDbVersion": "10.3.28",
  "TelegramApiKey": "110201543:AAHdqTcvCH1vGWJxfSeofSAs0K5PALDsaw",
  "ConnectionStrings": {
    "MainDatabase": "Server=127.0.0.1;Database=Table;User=User;Password=Password;TreatTinyAsBoolean=true;"
  },
  "AllowedHosts": "*"
}
